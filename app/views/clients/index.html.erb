<div class="content-div">
  <h1>Listing Clients</h1>

  <table>
    <thead>
      <tr>
        <th colspan="3"></th>
      </tr>
    </thead>
  
    <tbody>
      <% @clients.each do |client| %>
        <tr>
          <%= client.email %>
          <td><%= link_to 'Show', client_path(client) %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
  
  <br>
</div>

